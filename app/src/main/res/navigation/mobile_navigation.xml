<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@id/navigation_notifications">

    <fragment
        android:id="@+id/navigation_notifications"
        android:name="com.example.anniv_2022_am.ui.notifications.FragmentNotifications"
        android:label="@string/nav_notifications"
        tools:layout="@layout/fragment_navigation_notifications" />
    <fragment
        android:id="@+id/navigation_terminal"
        android:name="com.example.anniv_2022_am.ui.terminal.FragmentTerminal"
        android:label="@string/nav_terminal"
        tools:layout="@layout/fragment_navigation_terminal" />
    <fragment
        android:id="@+id/navigation_fichiers"
        android:name="com.example.anniv_2022_am.ui.fichiers.FragmentFichiers"
        android:label="@string/nav_fichiers"
        tools:layout="@layout/fragment_navigation_fichiers" />
    <fragment
        android:id="@+id/navigation_succes"
        android:name="com.example.anniv_2022_am.ui.succes.FragmentSucces"
        android:label="@string/nav_succes"
        tools:layout="@layout/fragment_navigation_succes" />
</navigation>